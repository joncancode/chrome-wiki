{
    "manifest_version": 2,
    "name": "Wiki-Helper",
    "version": "1.0",
    "description": "An extension to help get wiki entries",
    "icons": {
        "128": "images/icon128.png",
        "48": "images/icon48.png",
        "16": "images/icon16.png"
    },
    "background": {
        "scripts": ["eventpage.js"],
        "persistent": false
    },
    "permissions": [
        "tabs", 
        "contextMenus"
    ]
}